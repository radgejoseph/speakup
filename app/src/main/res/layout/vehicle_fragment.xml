<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        app:srcCompat="@drawable/header_vehicle" />


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/header_space"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <View
                android:id="@+id/header_void"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                app:layout_constraintHorizontal_bias="0.0"
                tools:ignore="MissingConstraints" />

            <View
                android:id="@+id/jeep_field"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@color/layout_jeep"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/header_void"
                app:layout_constraintVertical_bias="0.0" />

            <View
                android:id="@+id/taxicle_field"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@color/layout_taxicle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/jeep_field"
                app:layout_constraintVertical_bias="0.0" />

            <View
                android:id="@+id/tricycle_field"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@color/layout_tricycle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/taxicle_field"
                app:layout_constraintVertical_bias="0.0" />

            <View
                android:id="@+id/taxi_field"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:background="@color/layout_taxi"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tricycle_field"
                app:layout_constraintVertical_bias="0.0" />


            <ImageView
                android:id="@+id/jeep_image"
                android:layout_width="145dp"
                android:layout_height="151dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="238dp"
                app:layout_constraintBottom_toBottomOf="@+id/jeep_field"
                app:layout_constraintEnd_toEndOf="@+id/jeep_field"
                app:layout_constraintStart_toStartOf="@+id/jeep_field"
                app:layout_constraintTop_toTopOf="@+id/jeep_field"
                app:srcCompat="@drawable/jeepney_ico" />

            <ImageView
                android:id="@+id/taxicle_image"
                android:layout_width="102dp"
                android:layout_height="107dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="238dp"
                app:layout_constraintBottom_toBottomOf="@+id/taxicle_field"
                app:layout_constraintEnd_toEndOf="@+id/taxicle_field"
                app:layout_constraintStart_toStartOf="@+id/taxicle_field"
                app:layout_constraintTop_toTopOf="@+id/taxicle_field"
                app:srcCompat="@drawable/taxicle_ico" />

            <ImageView
                android:id="@+id/tricycle_image"
                android:layout_width="145dp"
                android:layout_height="151dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="238dp"
                app:layout_constraintBottom_toBottomOf="@+id/tricycle_field"
                app:layout_constraintEnd_toEndOf="@+id/tricycle_field"
                app:layout_constraintStart_toStartOf="@+id/tricycle_field"
                app:layout_constraintTop_toTopOf="@+id/tricycle_field"
                app:srcCompat="@drawable/tricycle_ico" />

            <ImageView
                android:id="@+id/taxi_image"
                android:layout_width="102dp"
                android:layout_height="107dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="238dp"
                app:layout_constraintBottom_toBottomOf="@+id/taxi_field"
                app:layout_constraintEnd_toEndOf="@+id/taxi_field"
                app:layout_constraintStart_toStartOf="@+id/taxi_field"
                app:layout_constraintTop_toTopOf="@+id/taxi_field"
                app:srcCompat="@drawable/taxi_ico" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="JEEPNEY"
                android:textColor="@color/buttonBlue"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/jeep_field"
                app:layout_constraintEnd_toEndOf="@+id/jeep_field"
                app:layout_constraintHorizontal_bias="0.641"
                app:layout_constraintStart_toStartOf="@+id/jeep_field"
                app:layout_constraintTop_toTopOf="@+id/jeep_field"
                app:layout_constraintVertical_bias="0.491" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TAXICLE"
                android:textColor="@color/buttonBlue"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/taxicle_field"
                app:layout_constraintEnd_toEndOf="@+id/taxicle_field"
                app:layout_constraintHorizontal_bias="0.641"
                app:layout_constraintStart_toStartOf="@+id/taxicle_field"
                app:layout_constraintTop_toTopOf="@+id/taxicle_field"
                app:layout_constraintVertical_bias="0.491" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TRICYCLE"
                android:textColor="@color/buttonBlue"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/tricycle_field"
                app:layout_constraintEnd_toEndOf="@+id/tricycle_field"
                app:layout_constraintHorizontal_bias="0.641"
                app:layout_constraintStart_toStartOf="@+id/tricycle_field"
                app:layout_constraintTop_toTopOf="@+id/tricycle_field"
                app:layout_constraintVertical_bias="0.491" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TAXI"
                android:textColor="@color/buttonBlue"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@+id/taxi_field"
                app:layout_constraintEnd_toEndOf="@+id/taxi_field"
                app:layout_constraintHorizontal_bias="0.641"
                app:layout_constraintStart_toStartOf="@+id/taxi_field"
                app:layout_constraintTop_toTopOf="@+id/taxi_field"
                app:layout_constraintVertical_bias="0.491" />


            <androidx.constraintlayout.widget.Group
                android:id="@+id/group_jeepney"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:constraint_referenced_ids="jeep_field,jeep_image,textView5" />

            <androidx.constraintlayout.widget.Group
                android:id="@+id/group_taxicle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:constraint_referenced_ids="taxicle_image,textView6,taxicle_field" />

            <androidx.constraintlayout.widget.Group
                android:id="@+id/group_tricycle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:constraint_referenced_ids="tricycle_field,tricycle_image,textView7" />

            <androidx.constraintlayout.widget.Group
                android:id="@+id/group_taxi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:constraint_referenced_ids="taxi_image,taxi_field,textView8" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>

</LinearLayout>